  "learning_outcomes": {
    "clos": [
      { "clo_id": "CLO1", "description": "Describe list data structures and implement singly linked lists." },
      { "clo_id": "CLO2", "description": "Define and implement stack and queue data structures." },
      { "clo_id": "CLO3", "description": "Describe and implement recursion and recursive algorithms." },
      { "clo_id": "CLO4", "description": "Explain trees, binary trees and Binary Search Trees, and implement BST operations." },
      { "clo_id": "CLO5", "description": "Discuss graphs and their applications, and implement basic graph operations." },
      { "clo_id": "CLO6", "description": "Explain the operation and performance of sorting algorithms." },
      { "clo_id": "CLO7", "description": "Explain hashing techniques and applications." },
      { "clo_id": "CLO8", "description": "Describe text processing problems and compression algorithms such as Huffman, LZW and Run-length encoding." }
    ]
  },

